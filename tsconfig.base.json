{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "rootDir": "src"
  },
  "include": ["src/**/*"]
}
